VENDOR=Genua
NAME=Genuscreen
IMAGE_FORMAT=iso
IMAGE_GLOB=*.iso

# Match versions like:
# genuscreen-5.0.1.iso
VERSION=$(shell echo $(IMAGE) | sed -e 's/genuscreen-\(.*\)\.iso/\1/')

-include ../makefile-sanity.include
-include ../makefile.include

ifeq ($(INSTALL),false)
$(info Install mode disabled)
else
$(info Install mode enabled)
-include ../makefile-install.include
endif
